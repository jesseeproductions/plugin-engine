{
  "name": "jeseeproductions/plugin-engine",
  "description": "Common classes and functions for plugin engine",
  "type": "wordpress-plugin",
  "keywords": [
    "wordpress",
    "coupons",
    "menu"
  ],
  "homepage": "http://jesseeproductions.com/",
  "license": "GPL-2.0",
  "require-dev": {
    "lucatume/wp-browser": "2.2.15",
    "codeception/codeception": "^2.5.6",
    "phpunit/phpunit": "6.5.14",
    "facebook/webdriver": "1.6.0",
    "lucatume/function-mocker": "^1.3.8",
    "lucatume/function-mocker-le": "^1.0.1"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "preferred-install": "dist"
  },
  "require": {
    "lucatume/di52": "~2.0",
    "faction23/a11y-dialog": "~5.0.0"
  },
  "repositories": [
    {
      "type": "package",
      "package": {
        "name": "faction23/a11y-dialog",
        "version": "5.0.4",
        "source": {
          "url": "https://github.com/faction23/a11y-dialog",
          "type": "git",
          "reference": "master"
        }
      }
    }
  ],
  "scripts": {
    "post-install-cmd": [
      "xrstf\\Composer52\\Generator::onPostInstallCmd"
    ],
    "post-update-cmd": [
      "xrstf\\Composer52\\Generator::onPostInstallCmd"
    ],
    "post-autoload-dump": [
      "xrstf\\Composer52\\Generator::onPostInstallCmd"
    ]
  },
  "autoload": {
    "psr-4": {
      "Pngx\\": "src/Pngx/"
    }
  }
}
